{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "execute_address",
    "operation",
    "query_address"
  ],
  "properties": {
    "execute_address": {
      "$ref": "#/definitions/AndrAddress"
    },
    "operation": {
      "$ref": "#/definitions/Operators"
    },
    "query_address": {
      "$ref": "#/definitions/AndrAddress"
    }
  },
  "definitions": {
    "AndrAddress": {
      "type": "object",
      "required": [
        "identifier"
      ],
      "properties": {
        "identifier": {
          "description": "Can be either an address or identifier of an ADO in a app.",
          "type": "string"
        }
      }
    },
    "Operators": {
      "type": "string",
      "enum": [
        "Greater",
        "GreaterEqual",
        "Equal",
        "LessEqual",
        "Less"
      ]
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "can_unwrap",
    "cw721_instantiate_type",
    "primitive_contract"
  ],
  "properties": {
    "can_unwrap": {
      "description": "Whether or not the cw721 token can be unwrapped once it is wrapped.",
      "type": "boolean"
    },
    "cw721_instantiate_type": {
      "description": "The cw721 contract can be instantiated or an existing address can be used. In the case that an existing address is used, the minter must be set to be this contract.",
      "allOf": [
        {
          "$ref": "#/definitions/InstantiateType"
        }
      ]
    },
    "kernel_address": {
      "type": [
        "string",
        "null"
      ]
    },
    "primitive_contract": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "definitions": {
    "Cw721Specification": {
      "type": "object",
      "required": [
        "name",
        "symbol"
      ],
      "properties": {
        "modules": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/Module"
          }
        },
        "name": {
          "type": "string"
        },
        "symbol": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "InstantiateType": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "new"
          ],
          "properties": {
            "new": {
              "$ref": "#/definitions/Cw721Specification"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      ]
    },
    "Module": {
      "description": "A struct describing a token module, provided with the instantiation message this struct is used to record the info about the module and how/if it should be instantiated",
      "type": "object",
      "required": [
        "address",
        "is_mutable"
      ],
      "properties": {
        "address": {
          "type": "string"
        },
        "is_mutable": {
          "type": "boolean"
        },
        "module_name": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "additionalProperties": false
    }
  }
}
